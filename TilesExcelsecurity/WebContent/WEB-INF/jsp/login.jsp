<html><head>
<title>SBI</title>
<meta http-equiv="Content-Type" content="text/html; charset=iso-8859-1">
<link href="css/sbi_css.css" rel="stylesheet" type="text/css">
<script language="JavaScript" src="script/sbi.js" type="text/javascript"></script>
</head>

<body>
<form action="loginprocess.do" method="post">
<table height="70%" border="0" cellspacing="0" cellpadding="0">
  <tbody><tr> 
    <td class="logo">&nbsp; </td>
  </tr>
  <tr> 
    <td align="center"><table class="login_table" border="0" cellspacing="0" cellpadding="0">
        <tbody><tr> 
          <td class="login_bar">Login</td>
        </tr>
        
        <tr> 
          <td class="login_con"> <table height="100%" border="0" cellspacing="0" cellpadding="0">
              <tbody><tr> 
                <td class="login_bg_left"><img width="1" src="../images/dot.gif"></td>
                <td class="login_bg_right"><img width="1" src="../images/dot.gif"></td>
              </tr>
              <tr> 
                <td class="login_left_bg_con">
					<table class="login_container" border="0" cellspacing="3" cellpadding="0">
                    <tbody><tr> 
                      <td>Login ID</td>
                      <td><input type="text" name="username">
                      </td>
                    </tr>
                    <tr> 
                      <td>&nbsp;</td>
                      <td>(ex.:dataentry,supervisor,admin)</td>
                    </tr>
                    <tr> 
                      <td>Password</td>
                      <td><input type="password" name="password">
                      </td>
                    </tr>
                    <tr>
                      <td>&nbsp;</td>
                      <td>(ex.:admin)</td>
                    </tr>
                    <tr> 
                      <td>&nbsp;</td>
                      <td ><input type="submit" value="login"><td>
                    </tr>
                    <tr> 
                      <td>&nbsp;</td>
                      <td><a class="link" href="#">Forgot Password</a> &nbsp; 
                        <a class="link" href="#">Reset Password</a></td>
                    </tr>
                  </tbody></table>
				
				</td>
                <td class="login_bg_right_mid">&nbsp;</td>
              </tr>
              <tr>
                <td class="login_left_bg"><img width="1" src="../images/dot.gif"></td>
                <td class="login_bg_right_dw"><img width="1" src="../images/dot.gif"></td>
              </tr>
            </tbody></table></td>
        </tr>
      </tbody></table></td>
  </tr>
</tbody></table>


</form>
</body></html>